<!-- ==================== PROPERTIES ==================== -->
<!-- Define limits as xacro properties for easy adjustment -->
<xacro:property name="velocity_limit" value="100.0"/>
<xacro:property name="effort_limit" value="100.0"/>

<xacro:arg name="use_mock_hardware" default="true"/>
<xacro:arg name="robot_serial_port" default="/dev/ttyUSB0"/>
<xacro:arg name="robot_baud_rate" default="1000000"/>
<xacro:arg name="hardware_interface" default="your_hardware/YourRobotHardware"/>
<xacro:arg name="sts_ids" default="1 2 3 4 5 6 7"/>
<xacro:arg name="sts_direction" default="-1 1 -1 -1 -1 -1 -1"/>
<xacro:arg name="sts_center_ticks" default="2048 2048 2048 2048 2048 2048 2048"/>
<xacro:arg name="sts_ticks_per_rev" default="4096"/>

<ros2_control name="lerobot_101_6_salut_controller" type="system">
  <hardware>
    <xacro:if value="$(arg use_mock_hardware)">
      <plugin>mock_components/GenericSystem</plugin>
    </xacro:if>
    <xacro:unless value="$(arg use_mock_hardware)">
      <plugin>$(arg hardware_interface)</plugin>
      <param name="serial_port">$(arg robot_serial_port)</param>
      <param name="baudrate">$(arg robot_baud_rate)</param>
      <param name="ids">$(arg sts_ids)</param>
      <param name="direction">$(arg sts_direction)</param>
      <param name="center_ticks">$(arg sts_center_ticks)</param>
      <param name="ticks_per_rev">$(arg sts_ticks_per_rev)</param>
    </xacro:unless>
  </hardware>
  <joint name="Joint1">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
  <joint name="Joint2">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
  <joint name="Joint3">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
  <joint name="Joint4">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
  <joint name="Joint5">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
  <joint name="Joint6">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
  <joint name="Joint7Gripper">
    <command_interface name="position"/>
    <state_interface name="position"/>
    <state_interface name="velocity"/>
  </joint>
</ros2_control>

